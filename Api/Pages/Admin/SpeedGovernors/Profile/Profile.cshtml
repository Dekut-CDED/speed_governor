<div class="border backgroundWhite container">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Location for SpeedgovenorA</h2>
        </div>

        <div class="col-6 text-right">
            <a asp-page="/Account/Register" class="btn btn-primary"> <i class="fas fa-plus"></i> &nbsp; Register New</a>
        </div>
    </div>
    <br> <br />

    <table id="Dt_Load" class="table table-striped table-bordered" style="width: 100%;">
